{
  "name": "docquery",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "serve": "vite build && node -r dotenv/config build",
    "serve:prod": "node -r dotenv/config build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "guideline_dl": "ts-node-esm src/weaviate_import/guideline_dl.ts",
    "weaviate_import": "ts-node-esm src/weaviate_import/import.ts",
    "nccn": "ts-node-esm src/weaviate_import/nccn_fetch.ts"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.2.8",
    "@iconify/svelte": "^3.1.3",
    "@nathanpb/progress": "^3.1.0",
    "@playwright/test": "^1.28.1",
    "@qdrant/js-client-rest": "^1.2.2",
    "@svelte-put/shortcut": "^2.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.3.0",
    "@sveltejs/kit": "^1.21.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/progress": "^2.0.5",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@zerodevx/svelte-toast": "^0.9.3",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.1.1",
    "dayjs": "^1.11.7",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.26.0",
    "fast-xml-parser": "^4.2.5",
    "graphql": "^16.6.0",
    "pocketbase": "^0.15.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "progress": "^2.0.3",
    "sass": "^1.69.0",
    "svelecte": "^3.16.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-copy-to-clipboard": "^0.2.5",
    "svelte-lightbox": "^1.1.2",
    "tailwindcss": "^3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vite-node": "^0.32.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "better-sqlite3": "^8.7.0",
    "dotenv": "^16.3.1"
  }
}
